<Template>
  <Audio>
    <Rocket>maze/data/sounds/rocket_launcher.ogg</Rocket>
  </Audio>
  <Render>
    <MaterialType>solid</MaterialType>
    <MaterialFlagLighting>true</MaterialFlagLighting>
    <AniMesh>data/shapes/character/sydney.md2</AniMesh>
    <CastsShadow>false</CastsShadow>
    <Texture0>data/shapes/character/sydney.bmp</Texture0>
    <Scale>0.2 0.2 0.2</Scale>
    <DrawLabel>true</DrawLabel>
    <Collision>5</Collision>
    <FPSCamera>
        <attach_point>2.5 0 0</attach_point>
        <target>50 0 0</target>
        <near_plane>0.1</near_plane>
        <far_plane>1000</far_plane>
    </FPSCamera>
  </Render>
</Template>
